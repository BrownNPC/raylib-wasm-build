{
  "name": "raylib-wasm",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "description": "Make raylib games in JS for the web",
  "scripts": {
    "start": "npx -y live-server site",
    "build:wasm": "docker run --rm -v $(pwd):/src -v /tmp/emscripten-cache:/emsdk/upstream/emscripten/cache/ -u $(id -u):$(id -g) emscripten/emsdk ./build.sh",
    "build:interface": "node gen_host.js",
    "publish": "npx -y surge site raylib.surge.sh",
    "build": "npm run build:wasm && npm run build:interface"
  },
  "keywords": [],
  "author": "David Konsumer <konsumer@jetboystudio.com>",
  "license": "MIT"
}
